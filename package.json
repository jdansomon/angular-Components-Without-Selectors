{
  "name": "no-selectors",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:stats": "ng build --stats-json && webpack-bundle-analyzer dist/no-selectors/stats.json",
    "esbuild": "esbuild --bundle --minify --sourcemap --target=es2015 --platform=browser --outfile=dist/no-selectors/main.js ./src/main.ts",
    "build:prod": "ng build --configuration production && npm run esbuild",
    "esbuild:analyze": "esbuild --bundle --minify --sourcemap --target=es2015 --platform=browser --outfile=dist/no-selectors/main.js --metafile=dist/no-selectors/metafile.json ./src/main.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "~17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/jasmine": "~4.3.0",
    "esbuild": "0.19.8",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
